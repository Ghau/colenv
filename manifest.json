{
  "manifest_version": 2,
  "name": "colenv",
  "version": "0.2",
  "description": "Colenv",
  "icons": {
    "32": "resources/icon_32.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "theme",
    "<all_urls>"
  ],
  "applications": {
    "gecko": {
      "id": "colenv-blog@alex"
    }
  }
}
