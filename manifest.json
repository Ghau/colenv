{
  "manifest_version": 2,
  "name": "colenv",
  "version": "0.1",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "resources/images/icon_48.png"
  },
  "background": {
    "scripts": ["browser-polyfill.js", "background.js"]
  },
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true,
    "browser_style": true
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "theme",
    "downloads",
    "<all_urls>"
  ],
  "applications": {
    "gecko": {
      "id": "colenv@alex"
    }
  }
}
