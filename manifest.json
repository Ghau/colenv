{
  "manifest_version": 2,
  "name": "colenv",
  "version": "0.1",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "48": "resources/images/icon_48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "theme"
  ]
}
